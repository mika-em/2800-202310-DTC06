<!-- SAVED PROMPT PRESET PAGE -->
<!-- header and backbar -->
<%-include ("../partials/header-persona.ejs")%>
<%-include ("../partials/emptybackbar.ejs")%>
<div class="col text-center">
  <h1>SAVED PROMPT PRESET</h1>
</div>
</div> 
</div>
<!-- placeholder for saved prompt presets db -->
<div class="container-fluid" style="padding-top: 200px;">
  <form action="/persona/chat" method="post" class="text-center">
    <div class="row">
      <% for (var i=0; i<locals.savedPromptParameter.length; i++) { %>
      <div class="col-12">
        <div class="card text-center">
          <div class="card-body">
            <label class="card-title"><%- i + "&nbsp;" + locals.savedPromptParameter[i] %></label><br>
            <input class="form-control" type="text" name="<%- locals.savedPromptParameter[i] %>" placeholder="Enter <%- locals.savedPromptParameter[i] %>" />
          </div>
        </div>
      </div>
      <% } %>
    </div>
    <br>
    <button class="btn" type="submit">Send</button>
  </form>
</div>

<!-- navbar and footer -->
<%-include ("../partials/navbar.ejs")%>
<%-include ("../partials/footer.ejs")%>